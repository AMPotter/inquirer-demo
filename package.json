{
  "name": "inquirer-demo",
  "version": "1.0.0",
  "description": "I'm going to try out Inquirer from NPM, it seems fun.",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "lint": "eslint .",
    "mocha": "mocha --recursive",
    "pretest": "npm run lint",
    "test": "npm run mocha -s",
    "test:unit": "npm run mocha -- ./test/unit",
    "test:e2e": "npm run mocha -- ./test/e2e",
    "test:watch": "nodemon --exec \"npm run mocha\" -s",
    "test:watch:unit": "nodemon --exec \"npm run mocha -s -- ./test/unit\"",
    "test:watch:e2e": "nodemon --exec \"npm run mocha -s -- ./test/e2e\"",
    "start": "node server.js",
    "start:watch": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AMPotter/inquirer-demo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AMPotter/inquirer-demo/issues"
  },
  "homepage": "https://github.com/AMPotter/inquirer-demo#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^5.2.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.16.3",
    "inquirer": "^6.0.0",
    "jsonfile": "^4.0.0",
    "jsonwebtoken": "^8.3.0",
    "mkdirp": "^0.5.1",
    "mock-stdin": "^0.3.1",
    "mongoose": "^5.2.5",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.2",
    "shortid": "^2.2.12",
    "std-mocks": "^1.0.1",
    "superagent": "^3.8.3",
    "terminal-kit": "^1.19.2"
  }
}
